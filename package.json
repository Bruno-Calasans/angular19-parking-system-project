  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "component": "ng g c --skip-tests --inline-style",
    "format": "prettier --write \"src/**/*.{ts,html,scss}\"",
    "lint": "ng lint",
    "prepare": "husky install"
